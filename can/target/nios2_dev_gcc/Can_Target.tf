$ ====================================================================
$     A-CANターゲット依存テンプレート（NIOS2_DEV用）
$ ====================================================================

$ ====================================================================
$ 非依存部向け定義
$ ====================================================================
$ // コントローラ最大数
$TARGET_TMAX_CONTROLLER_NUM = 1$

$ // フィルタマスク最大数
$TARGET_TMAX_FILTERMASK_NUM = 32$

$ // サポートボーレート
$TARGET_SUPPORT_BAUDRATE_LIST = {500, 250}$

$ ====================================================================
$ 依存部向け定義
$ ====================================================================
$ // 受信メールボックス最大数
$TMAX_RX_MB_NUM = 32$

$ // 送信メールボックス最大数
$TMAX_TX_MB_NUM = 16$

$ ====================================================================
$ [CAN022][CAN024] 依存部判定関数
$ ====================================================================
$ // メールボックス数チェック
$FUNCTION CHECK_MAILBOX_NUM$
	$IF ((ARGV[2] <= TMAX_RX_MB_NUM) && (ARGV[3] <= TMAX_TX_MB_NUM))$
		$RESULT = 1$
	$ELSE$
		$RESULT = 0$
	$END$
$END$

$ ====================================================================
$  標準テンプレートファイルのインクルード
$ ====================================================================
$INCLUDE "can.tf"$
